#is2
{
    margin:20px 35px 0px 35px;
}

#is2 *
{
    -webkit-font-smoothing:antialiased;
    font-family: Helvetica,Arial,sans-serif
}

#is2 a
{
    color:#0f9cc9;
    outline:none
}

#is2 a::-moz-focus-inner
{
    border:0
}

#is2 a:active,#is2 a:hover
{
    text-decoration:underline
}

#is2 a:link,#is2 a:visited
{
    text-decoration:none
}

#is2 h2
{
    margin-top:0;
    text-align:left
}

#is2 h1
{
    clear:left;
    color:#333;
    display:block;
    font-size:1.9em;
    margin:0;
    padding:0
}

#is2 p
{
    margin:0;
    padding:0
}

#is2 .clear
{
    clear:both
}

#is2 div.clearfix
{
    clear:both;
    margin-left:0;
    visibility:hidden
}

#is2 br.clear
{
    clear:both;
    height:0;
    line-height:0;
    margin:0;
    padding:0;
    width:0
}

#is2 #more_info
{
    height:20px;
    padding:12px 0;
    text-align:right
}

#is2 #more_info a
{
    color:#00B3E9;
    font-weight:700;
    -webkit-transition:color .2s ease-in-out,color .2s ease-in-out;
    -moz-transition:color .2s ease-in-out,color .2s ease-in-out;
    -ms-transition:color .2s ease-in-out,color .2s ease-in-out;
    transition:color .2s ease-in-out,color .2s ease-in-out
}

#is2 #more_info a:hover
{
    color:#5FBA50;
    -webkit-transition:color .2s ease-in-out,color .2s ease-in-out;
    -moz-transition:color .2s ease-in-out,color .2s ease-in-out;
    -ms-transition:color .2s ease-in-out,color .2s ease-in-out;
    transition:color .2s ease-in-out,color .2s ease-in-out
}

#is2 .empty
{
    color:#D3D3D3
}

#is2 .tooltip
{
    -webkit-border-radius:2px;
    -moz-border-radius:2px;
    border-radius:2px;
    -moz-box-shadow:0 1px 5px 0 rgba(20,20,20,0.7),inset 1px 1px 0 0 #222,inset -1px -1px 0 0 #222;
    -webkit-box-shadow:0 1px 5px 0 rgba(20,20,20,0.7),inset 1px 1px 0 0 #222,inset -1px -1px 0 0 #222;
    box-shadow:0 1px 5px 0 rgba(20,20,20,0.7),inset 1px 1px 0 0 #222222,inset -1px -1px 0 0 #222;
    background-color:#111;
    border:1px solid #000;
    color:#FFF;
    clear:both;
    display:none;
    font-size:12px;
    margin:10px;
    max-width:22%;
    padding:10px 15px;
    position:relative;
    text-align:left;
    text-shadow:0 1px 0 #000;
    white-space:normal;
    z-index:2001
}

#is2 .cta_button
{
    background:#5FBA50;
    background-color:#5FBA50;
    background-position:91% center;
    background-repeat:no-repeat;
    border:1px solid rgba(0,0,0,.1);
    border-bottom:1px solid rgba(0,0,0,.3);
    border-radius:2px;
    box-shadow:rgba(0,0,0,0.2) 0 1px 2px 0;
    -moz-border-radius:2px;
    -webkit-border-radius:2px;
    color:#fff;
    display:inline-block;
    float:right;
    font-size:12px;
    font-weight:700;
    margin:0;
    margin-left:30px;
    padding:12px 31px 12px 16px;
    text-decoration:none!important;
    text-shadow:0 1px 0 rgba(0,0,0,.4);
    transition:background-color .2s ease-in-out,color .2s ease-in-out;
    -moz-transition:background-color .2s ease-in-out,color .2s ease-in-out;
    -ms-transition:background-color .2s ease-in-out,color .2s ease-in-out;
    -webkit-transition:background-position 0 none;
    -webkit-transition:background-color .2s ease-in-out,color .2s ease-in-out;
    vertical-align:middle
}

#is2 .cta_button:hover
{
    background-color:#00B3E9;
    cursor:pointer
}

#is2 .cta_button.unlock_now_button
{
    background-color:#00B3E9
}

#is2 .cta_button.unlock_now_button:hover
{
    background-color:#5FBA50
}

#is2 .vertical_aligner
{
    display:inline-block;
    height:100%;
    vertical-align:middle
}

/******* Modal Style *******/

#is2 .header_text
{
    display:block;
    font-family: 'Roboto',sans-serif;
    font-size: 2.4em;
    font-weight: 900;
    margin-bottom: 8px;
    padding: 10px 10px
}

#is2 .subheader_text, .conf_subheader_text
{
    color:#666;
    display:block;
    font-family:'Roboto',sans-serif;
    font-size:1.7em;
    font-weight:400;
    margin-bottom:35px;
    padding:0 10px
}

#is2 .conf_subheader_text
{
    background-color: #333333;
    color: #fff;
    padding: 9px 0px 9px 5px;
    margin-bottom: 20px;
}

#is2 .modal_container
{
    filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#b300b3e9',endColorstr='#b3000000',GradientType=0);
    bottom:0;
    display:none;
    left:0;
    position:absolute;
    right:0;
    top:0
}

#is2 .modal
{
    display:table;
    overflow:auto;
    background:#fff;
    border:5px solid #333333;
    box-shadow:0 3px 40px 10px rgba(0,0,0,.4);
    margin:0 auto;
    padding:0px 0px 0px 0px;
    position:relative;
    top:200px;
    max-height:600px;
    z-index:10
}

#is2 .modal .modal_inner
{
    display:table
}

#is2 .modal .large
{
    padding-bottom:30px;
    width:1100px
}

#is2 .modal .cta_button
{
    font-size:13px;
    padding:10px 28px 10px 9px;
    position:absolute;
    right:45px;
    text-align:center;
    top:62px;
    width:84px
}

#is2 .modal .cta_button.datepicker_cta
{
    position:absolute;
    top:62px;
    margin: 0 0 10px 0;
}

#is2 .modal .modal_footer
{
    background:#eee;
    border-top:2px solid #d4d4d4;
    bottom:0;
    left:0;
    padding:18px 0;
    position:absolute;
    width:100%
}

#is2 .modal .modal_footer .modal_logo
{
    display:inline-block;
    float:right;
    height:39px;
    margin-right:30px;
    width:263px
}

#is2 .modal .modal_footer .modal_logo_label
{
    color:#666;
    display:inline-block;
    float:right;
    font-family:'Roboto',sans-serif;
    font-size:1.2em;
    font-style:italic;
    font-weight:300;
    margin-right:28px;
    margin-top:9px;
    vertical-align:center
}

#is2 .modal .modal_close
{
    -webkit-transition:all .2s ease-in-out;
    -moz-transition:all .2s ease-in-out;
    -ms-transition:all .2s ease-in-out;
    transition:all .2s ease-in-out;
    padding:9px;
    opacity:.5;
    position:absolute;
    right:5px;
    top:0px
}

#is2 .modal .modal_close:hover
{
    cursor:pointer;
    opacity:1
}

#date-picker-img
{
    padding: 0px 0px 65px 0px;
}

/******* Pivot Style *******/

#is2 .pivots
{
    position:relative;
    width:100%
}

#is2 .pivots .pivot
{
    display:none;
    min-height:740px;
    width:100%
}

#is2 .pivot_demo
{
    margin:0 auto;
    margin-bottom:50px;
    padding-bottom:40px;
    text-align:center;
    width:900px
}

#is2 .pivot_demo h1
{
    margin-top:35px;
    text-align:center
}

#is2 .pivot_demo h2
{
    text-align:center
}

#is2 .pivot_demo .demo_cta_button_container
{
    display:inline-block;
    vertical-align:middle;
    margin-bottom:35px
}

#is2 .pivot_demo_footer
{
    background:#eee;
    border-top:8px solid #00b3e9;
    height:220px;
    text-align:center;
    width:100%
}

#is2 .pivot_demo_footer .footer_container
{
    display:inline-block;
    vertical-align:middle
}

#is2 .pivot_demo_footer .footer_container .footer_logo_container
{
    text-align:left
}

#is2 .pivot_demo_footer .logo
{
    display:inline-block;
    height:50px;
    margin-right:30px;
    vertical-align:middle;
    width:263px
}

#is2 .pivot_demo_footer .logo_label
{
    color:#666;
    display:inline-block;
    font-family:'Roboto',sans-serif;
    font-size:1.4em;
    font-style:italic;
    font-weight:300;
    margin-bottom:22px;
    margin-left:60px;
    vertical-align:center
}

#is2 .pivot_demo_footer .footer_cta_button_container .cta_button
{
    float:none
}

#is2 .screenshot
{
    border: 5px solid #00b3e9;
    cursor:pointer
}

#is2 .excluded_opps_msg_container
{
    float:left;
    margin-left:9px
}

#is2 .excluded_opps_msg
{
    color:#555;
    font-family:"Roboto"
}

#is2 .excluded_opps_msg_num
{
    font-family:"Roboto";
    font-weight:900
}

#is2 .chart_error
{
    font-size:18px;
    color:#333;
    text-align:center;
    display:inline-block;
    width:100%;
    height:400px;
    line-height:400px
}

#is2 .chart_error .is_error_msg
{
    color:#0f9cc9;
    font-weight:700;
    cursor:pointer
}

#is2 .help_link
{
    float:right;
    padding-top:3px
}